{
    "Serilog": {
        "Using": [
            "Serilog.Sinks.Console"
        ],
        "MinimumLevel": {
            "Default": "Debug",
            "Override": {
                "Microsoft": "Warning",
                "System": "Warning"
            }
        },
        "WriteTo": [
            {
                "Name": "Console",
                "Args": {
                    "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3} {CorrelationId} {ClientId}] {Message:lj} {Sub} {NewLine}{Exception}"
                }
            },
            {
                "Name": "Seq",
                "Args": {
                    "serverUrl": "http://localhost:5341"
                }
            },
            {
                "Name": "OpenTelemetry",
                "Args": {
                    "endpoint": "http://localhost:4317",
                    "protocol": "Grpc"
                }
            }
        ]
    },
    "ConnectionStrings": {
        "Redis": "localhost:6379"
    },
    "OpenTelemetry": {
        "Otlp": {
            "Endpoint": "http://localhost:4317",
            "Protocol": "Grpc"
        }
    },
    "Jwt": {
        "Key": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJzdWIiOiIxMjM0NTY3ODkwIiwibmFtZSI6IkpvaG4gRG9lIiwiYWRtaW4iOnRydWUsImlhdCI6MTUxNjIzOTAyMn0.KMUFsIDTnFmyG3nMiGM6H9FNFUROf3wh7SmqJp-QV30",
        "Issuer": "https://yourdomain.com",
        "Audience": "https://yourdomain.com",
        "AccessExpireMinutes": 15,
        "RefreshExpireDays": 7
    },
    "FrankfurterApi": {
        "BaseUrl": "https://api.frankfurter.dev",
        "CacheLatestDurationMinutes": 1,
        "CacheHistoricalDurationHours": 1,
        "RetryPolicy": {
            "RetryCount": 3,
            "BaseBackoffSeconds": 2
        },
        "CircuitBreakerPolicy": {
            "FailuresBeforeBreaking": 12,
            "BreakDurationMinutes": 1
        }
    }
}